class Solution {
    public List<String> buildArray(int[] target, int n) {
        List<String> str = new ArrayList<>();
        int len = target.length;
        int i = 0;
        int number = 1;
        while(i < len){
            str.add("Push");
            if(target[i] != number) str.add("Pop"); 
            else i++;
            number++;
        }
        return str;
    }
}
